{
	"version": "1.0.0-beta-5",
	"dependencies": {
		"DotLiquidForkForZKWeb": "1.0.2",
		"NETStandard.Library": "1.6.0",
		"Newtonsoft.Json": "9.0.1",
		"npgsql": "3.1.5",
		"ZKWeb": "1.0.0-beta-5",
		"ZKWebStandard": "1.0.0-beta-5"
	},
	"frameworks": {
		"net461": {
			"frameworkAssemblies": {
				"System.Drawing": "4.0.0.0"
			},
			"dependencies": {
				"FluentNHibernate": "2.0.3",
				"MySql.Data": "7.0.3-DMR",
				"NSubstitute": "1.10.0",
				"SQLiteForZKWeb": "1.0.102"
			}
		},
		"netcoreapp1.0": {
			"imports": [
				"dotnet5.6",
				"portable-net45+win8"
			],
			"buildOptions": { "define": [ "NETCORE" ] },
			"dependencies": {
				"Microsoft.NETCore.App": {
					"type": "platform",
					"version": "1.0.0"
				}
			}
		}
	},
	"buildOptions": {
		"emitEntryPoint": true,
		"optimize": true,
		"compile": { "exclude": "App_Data/*" }
	}
