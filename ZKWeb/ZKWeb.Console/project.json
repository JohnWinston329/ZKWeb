{
	"version": "1.1-beta-1",
	"dependencies": {
		"NETStandard.Library": "1.6.0",
		"Newtonsoft.Json": "9.0.1",
		"NSubstitute": "2.0.0-rc",
		"ZKWeb": "1.1-beta-1",
		"ZKWeb.ORM.Dapper": "1.1-beta-1",
		"ZKWeb.ORM.EFCore": "1.1-beta-1",
		"ZKWeb.ORM.InMemory": "1.1-beta-1",
		"ZKWeb.ORM.MongoDB": "1.1-beta-1",
		"ZKWebStandard": "1.1-beta-1"
	},
	"frameworks": {
		"net461": {
			"dependencies": {
				"ZKWeb.ORM.NHibernate": "1.1-beta-1"
			}
		},
		"netcoreapp1.0": {
			"imports": [
				"dotnet5.6",
				"portable-net45+win8"
			],
			"buildOptions": { "define": [ "NETCORE" ] },
			"dependencies": {
				"Microsoft.NETCore.App": {
					"type": "platform",
					"version": "1.0.1"
				}
			}
		}
	},
	"buildOptions": {
		"emitEntryPoint": true,
		"optimize": true,
		"compile": { "exclude": "App_Data/*" }
	}
}
