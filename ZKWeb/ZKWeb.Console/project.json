{
	"version": "1.5-beta-2",
	"dependencies": {
		"NETStandard.Library": "1.6.1",
		"Newtonsoft.Json": "9.0.1",
		"NSubstitute": "2.0.0-rc",
		"ZKWeb": "1.5-beta-2",
		"ZKWeb.ORM.Dapper": "1.5-beta-2",
		"ZKWeb.ORM.EFCore": "1.5-beta-2",
		"ZKWeb.ORM.InMemory": "1.5-beta-2",
		"ZKWeb.ORM.MongoDB": "1.5-beta-2",
